{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./master/dist",
    "rootDir": "./master/src"
  },
  "include": ["./master/src/**/*"],
  "exclude": ["./master/node_modules", "./dist"]
}
